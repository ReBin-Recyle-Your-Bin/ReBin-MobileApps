<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    tools:context=".view.storyInspiration.StoryInspirationActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/tblStoryInspiration"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:navigationIcon="@drawable/ic_arrow"
        app:showAsAction="ifRoom"
        app:title="Story Inspiration"
        app:titleTextColor="@color/black" />

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/rvStoryInspirationAll"
        app:layout_constraintTop_toBottomOf="@+id/tblStoryInspiration"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>